{
  "name": "torrentfilejs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir -c electron-builder.yml",
    "dist": "electron-builder -c electron-builder.yml",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "build:mainproc": "webpack-cli --mode=development --config webpack.mainproc.config.js",
    "lint": "vue-cli-service lint",
    "clean": "rm -rf ./dist && rm -rf ./bin && rm -rf ./dist.electron",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "electron-builder": "^23.0.3",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.4.1",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "typescript": "^4.6.4"
  }
}
